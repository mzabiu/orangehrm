<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE suite SYSTEM "https://testng.org/testng-1.0.dtd">
<suite name="Regression">
	<listeners>
		<listener
			class-name="com.orangehrm.framework.listeners.TestListeners" />
	</listeners>
	<test name="Regression_Test_EDGE">
		<parameter name="browser" value="edge" />
		<classes>
			<class name="com.orangehrm.testcases.login.LoginTestCase" />
		</classes>
	</test>

	<test name="Regression_Test_chrome">
		<parameter name="browser" value="chrome" />
		<classes>
			<class name="com.orangehrm.testcases.login.LoginTestCase" />
		</classes>
	</test>

	<test name="Regression_Test_firefox">
		<parameter name="browser" value="firefox" />
		<classes>
			<class name="com.orangehrm.testcases.login.LoginTestCase" />
		</classes>
	</test>
</suite>
